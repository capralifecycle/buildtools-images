{
  "extends": ["github>capraconsulting/renovate-config:default"],
  "pinDigests": true,
  "digest": {
    "automerge": true,
    "automergeType": "branch"
  },
  "packageRules": [
    {
      "groupName": "azul/zulu-openjdk-debian",
      "packagePatterns": ["^azul/zulu-openjdk-debian:"]
    },
    {
      "groupName": "azul/zulu-openjdk-alpine",
      "packagePatterns": ["^azul/zulu-openjdk-alpine:"]
    }
  ]
}
